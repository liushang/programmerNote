.extend{
    border: 1rpx solid red;
    height: 100%;
    position: relative;
    /* top: 0;
    left: -95%; */
    width: 94%;
    z-index: 2;
    /* padding-left: 40%; */
    background: #f5f5f5;
    box-sizing: border-box;
}
.catalogue{
    overflow-y: scroll;
}
.level-1{
    font-size: 28rpx;
    padding: 8rpx 0;
    line-height: 44rpx;
}
.level-1-choose{
    background: red;
}
.level-2{
    font-size: 24rpx;
    margin-left: 40rpx;
    line-height: 40rpx;
}
.level-2-choose{
    background: green;
}
.level-2-left{
    margin-left: 40rpx;
}
.level-3{
    margin-left: 40rpx;
    font-size: 20rpx;
    line-height: 40rpx;
}
.level-3-choose{
    background: yellow;
}
.level-input {
    border: 1rpx solid black;
    margin-top: 0;
    padding: 0;
    width: 60%;
}

.side-confirm{
    height: 60rpx;
    width: 40rpx;
    top: 90%;
    right: 2%;
    z-index: 3;
    position: absolute;
}
.inputs{
    height: 200rpx;
    position: absolute;
    bottom: 0;
    /* left: 40%; */
    width: 100%;
}